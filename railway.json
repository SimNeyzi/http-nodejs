{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "repo": "SimNeyzi/http-nodejs",
      "template": {
        "serviceName": "http-nodejs",
        "serviceSource": "https://github.com/railwayapp-templates/http-nodejs"
      }
    }
  },
  "deploy": {
    "numReplicas": 1,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
